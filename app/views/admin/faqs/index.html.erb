<% breadcrumb :admin_faqs, @faqs, style: :bootstrap %>
<div class="articles">
  <h2><%= @faqs.model_name %></h2>
  <div class="row">
    <div class="col-sm-12">
        <%= link_to "Add Faq", new_admin_faq_path, class: "btn btn-primary" %>
    </div>
  </div>
  <table class="table">
    <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
      <th></th>
    </tr>
    </thead>
    <tbody>
    <%= render @faqs %>
    </tbody>
  </table>
</div>