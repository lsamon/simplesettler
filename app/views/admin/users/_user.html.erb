<%= content_tag_for(:tr, user) do %>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td><%= user.admin %></td>
  <td><%= link_to(raw("<i class='fa fa-pencil-square-o'></i>"), edit_admin_user_path(user)) %></td>
  <td><%= link_to(raw("<i class='fa fa-trash'></i>"), admin_user_path(user), method: 'delete', remote: true) %></td>
<% end %>
