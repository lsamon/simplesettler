$(document).ready(function(){
  $('.articleSelect').remove();

  var $articleContainer = $('.articleSelectContainer .rows');

  console.log('hotdogs');

  <% @all_articles.each do |article| %>
    var $div = $('<div class="articleSelect">');
    $div.text("<%= article.title %>");
    $articleContainer.append($div);
  <% end %>

});
