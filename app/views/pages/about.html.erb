<% title @article&.meta_title.present? ? @article&.meta_title : @article&.title %>
<% meta_tag :description, @article&.meta_description %>
<% meta_tag :keywords, @article&.keywords %>
<div class="container">
  <div class="pages" id="startchange">
    <h1><%= @article&.title %></h1>
    <%= simple_format @article&.content %>
  </div>
</div>
