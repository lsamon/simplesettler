<div class="container">
  <%= form_for @article do |f| %>
    <%= f.text_field :title, :placeholder => 'Title', :class => 'articleEditTitle' %>
    <%= f.cktext_area :content, :class => '' %>
    <%= f.select :city_id, City.pluck(:name, :id), :class => 'articleEditCity' %>
    <%= f.submit :class => '' %>
  <% end %>
</div>
